{
   "name": "notivue",
   "version": "0.9.0",
   "private": false,
   "description": "Fully-fledged notification system for Vue 3.",
   "homepage": "https://notivue.netlify.app/",
   "bugs": {
      "url": "https://github.com/smastrom/notivue/issues"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/smastrom/notivue.git"
   },
   "license": "MIT",
   "author": {
      "name": "Simone Mastromattei",
      "email": "smastrom@proton.me"
   },
   "exports": {
      ".": {
         "import": "./dist/index.mjs",
         "require": "./dist/index.js"
      },
      "./notifications.css": {
         "import": "./dist/notifications.css",
         "require": "./dist/notifications.css"
      },
      "./animations.css": {
         "import": "./dist/animations.css",
         "require": "./dist/animations.css"
      }
   },
   "main": "dist/index.js",
   "module": "dist/index.mjs",
   "types": "dist/index.d.ts",
   "files": [
      "dist/*"
   ],
   "scripts": {
      "build": "rimraf dist && vue-tsc && vite build",
      "postbuild": "postcss ./src/animations.css ./src/notifications.css -d ./dist/ --use cssnano postcss-nesting --no-map",
      "build:demo": "rimraf dist && vite build --mode demo",
      "dev": "vite",
      "preview": "vite preview"
   },
   "devDependencies": {
      "@rollup/plugin-terser": "^0.4.1",
      "@vitejs/plugin-vue": "^4.1.0",
      "cssnano": "^5.1.15",
      "postcss": "^8.4.23",
      "postcss-cli": "^10.1.0",
      "postcss-nesting": "^11.2.2",
      "prettier": "^2.8.8",
      "rimraf": "^4.4.1",
      "typescript": "^4.9.5",
      "vite": "^4.3.1",
      "vite-plugin-dts": "^2.3.0",
      "vue": "^3.2.47",
      "vue-tsc": "^1.4.4"
   }
}
